   -from [get_clocks {FPGA_CLK1_50}] \
   -to [get_clocks {*:u0|*:miner_0|*:altera_pll_0|outclk_wire[0]}]

set_false_path \
   -to [get_clocks {FPGA_CLK1_50}] \
   -from [get_clocks {*:u0|*:miner_0|*:altera_pll_0|outclk_wire[0]}]



